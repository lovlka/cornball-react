{
  "author": "Victor Stodell",
  "name": "cornball-react",
  "description": "Cornball",
  "version": "1.1.0",
  "private": true,
  "homepage": "https://cornball.se",
  "repository": {
    "type": "git",
    "url": "git@github.com:lovlka/cornball-react.git"
  },
  "scripts": {
    "start": "npm-run-all --parallel start:**",
    "start:client": "webpack-dev-server --config ./webpack.dev.js",
    "start:server": "netlify-lambda serve ./functions -c ./webpack.functions.js",
    "build": "npm-run-all --parallel build:**",
    "build:client": "webpack --config ./webpack.prod.js",
    "build:server": "netlify-lambda build ./functions -c ./webpack.functions.js",
    "analyze": "webpack --config ./webpack.analyze.js",
    "lint": "eslint webpack.*.js ./src ./functions"
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "faunadb": "^2.8.0",
    "halogenium": "^2.3.0",
    "immutable": "^3.8.2",
    "interactjs": "^1.6.3",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-intl": "^2.9.0",
    "react-redux": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^2.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "netlify-lambda": "^1.6.3",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pre-commit": "^1.2.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.4.1",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  }
}
